<h1>Listing magic_urls</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Description</th>
    <th>Privacy</th>
    <th>User</th>
    <th>Active</th>
    <th>Type</th>
  </tr>

<% @magic_urls.each do |magic_url| %>
  <tr>
    <td><%=h magic_url.url %></td>
    <td><%=h magic_url.description %></td>
    <td><%=h magic_url.privacy %></td>
    <td><%=h magic_url.user_id %></td>
    <td><%=h magic_url.active %></td>
    <td><%=h magic_url.type %></td>
    <td><%= link_to 'Show', magic_url %></td>
    <td><%= link_to 'Edit', edit_magic_url_path(magic_url) %></td>
    <td><%= link_to 'Destroy', magic_url, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New magic_url', new_magic_url_path %>